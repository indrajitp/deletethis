<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="Jangomail Provider">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>Remove-Target-Path-suffix</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>http://api.jangomail.com/api.asmx/SendTransactionalEmail</URL>
        <Property name="io.timeout.millis">120000</Property>
    </HTTPTargetConnection>
</TargetEndpoint>